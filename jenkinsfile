pipeline {
    agent { label 'master'}
    options{
        timestamps()
           }
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }

        stage('Build'){
            options {
                timeout(time: 1, unit: "SECONDS")
            }
            steps {
                echo "Building"
                //sleep 5
            }
        }

        stage('Test'){
            steps{
                echo "Testing123"
                sh 'ssh --help'
                sh "git clone https://github.com/venkatkagga/test16.git"
            sh "ls"
            echo "checkout complete"
            sh "ssh -i stagelync.pem ubuntu@52.60.236.156"
            sh "whoami"   
            }
    }
}
}
