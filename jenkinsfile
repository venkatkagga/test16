pipeline {
    agent any
    options{
        timestamps()
           }
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }

        stage('Build'){
            options {
                timeout(time: 1, unit: "SECONDS")
            }
            steps {
                echo "Building"
                //sleep 5
            }
        }

        stage('Test'){
            steps{
                echo "Testing123"
                ssh --help
            }
        }
        stage('SSH Login'){
            steps{
                sshagent(credentials: ['Test123']){
                    sh 'ssh -v ubuntu@52.60.236.156'
                }
            }
        }
    }
}



